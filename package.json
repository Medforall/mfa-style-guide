{
    "name": "@dvukovic/style-guide",
    "version": "0.2.0",
    "files": [
        "src/eslint",
        "src/cspell",
        "src/prettier",
        "src/stylelint"
    ],
    "scripts": {
        "lint": "yarn lint:prettier && yarn lint:eslint && yarn lint:stylelint && yarn lint:spell",
        "lint:eslint": "eslint . --ext .js,.ts,.tsx",
        "lint:fix": "yarn lint:prettier --write && yarn lint:stylelint --fix && yarn lint:eslint --fix",
        "lint:prettier": "prettier --log-level=warn --check .",
        "lint:spell": "cspell --no-progress --no-summary --unique '**'",
        "lint:stylelint": "stylelint ./**/*.css",
        "release": "release-it"
    },
    "dependencies": {
        "@eslint-community/eslint-plugin-eslint-comments": "^4.3.0",
        "@next/eslint-plugin-next": "^14.2.5",
        "@prettier/plugin-xml": "^3.4.1",
        "@typescript-eslint/parser": "^7.17.0",
        "eslint-plugin-etc": "^2.0.3",
        "eslint-plugin-import-x": "^3.1.0",
        "eslint-plugin-jest": "^28.6.0",
        "eslint-plugin-jest-formatting": "^3.1.0",
        "eslint-plugin-mobx": "^0.0.10",
        "eslint-plugin-n": "^17.9.0",
        "eslint-plugin-promise": "^7.0.0",
        "eslint-plugin-react": "^7.35.0",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-security-node": "^1.1.4",
        "eslint-plugin-sonarjs": "^1.0.4",
        "eslint-plugin-sort-destructure-keys": "^2.0.0",
        "eslint-plugin-sort-keys-fix": "^1.1.2",
        "eslint-plugin-typescript-sort-keys": "^3.2.0",
        "eslint-plugin-unicorn": "^54.0.0",
        "eslint-plugin-unused-imports": "^4.0.1",
        "prettier-plugin-embed": "^0.4.15",
        "prettier-plugin-jsdoc": "^1.3.0",
        "prettier-plugin-packagejson": "^2.5.1",
        "prettier-plugin-prisma": "^5.0.0",
        "prettier-plugin-sh": "^0.14.0",
        "prettier-plugin-sql": "^0.18.1",
        "stylelint-order": "^6.0.4",
        "typescript-eslint": "^7.17.0"
    },
    "devDependencies": {
        "@release-it/conventional-changelog": "^8.0.1",
        "@total-typescript/tsconfig": "^1.0.4",
        "@types/eslint": "^8.56.11",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.14.12",
        "@types/react": "^18.3.3",
        "cspell": "^8.12.1",
        "eslint": "^8.57.0",
        "jest": "^29.7.0",
        "prettier": "^3.3.3",
        "react": "^18.3.1",
        "release-it": "^17.6.0",
        "stylelint": "^16.7.0",
        "typescript": "^5.5.4"
    },
    "peerDependencies": {
        "cspell": "8",
        "eslint": "8.57",
        "prettier": "3",
        "stylelint": "16"
    },
    "packageManager": "yarn@4.3.1",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    }
}
