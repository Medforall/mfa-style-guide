{
    "compilerOptions": {
        "jsx": "react",
        "target": "es2016",
        "module": "commonjs",
        "esModuleInterop": true,
        "noUncheckedIndexedAccess": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["src/**/*", "*.js", ".*.js"]
}
